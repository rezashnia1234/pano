<krpano onstart="startup();" showerrors="true" logkey="true">
	<action name="startup">
		loadscene("pano", null, MERGE);
	</action>
	<plugin name="gyro" devices="html5" keep="true"
			url="%SWFPATH%/plugins/gyro2.js" 
			enabled="false"
			camroll="true"
			friction="0.0"
			touch_mode="full"
			sensor_mode="1"
			autocalibration="true"
			onavailable=""
			onunavailable=""
			onenable=""
			ondisable=""
			/>
	<plugin name="gyro_activator" div_target="pano02" url="%SWFPATH%/images/list/activator.js" keep="true" devices="html5" />
			
	<scene name="pano" title="model" >
		<image >
			<sphere url="%SWFPATH%/images/list/sadabad-honar.jpg" />
		</image>
	</scene>
	
	
	
	
</krpano>
